<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <ageverification>
            <general>
                <enabled>0</enabled>
            </general>
        </ageverification>
        <ageverification>
            <general>
                <popup_width>auto</popup_width>
            </general>
        </ageverification>
        <ageverification>
            <general>
                <popup_height>auto</popup_height>
            </general>
        </ageverification>
        <ageverification>
            <general>
                <cookie_interval>24</cookie_interval>
            </general>
        </ageverification>
        <ageverification>
            <general>
                <popup_block>magentiz-age-verification</popup_block>
            </general>
        </ageverification>
        <ageverification>
            <general>
                <pages>home</pages>
            </general>
        </ageverification>
        <ageverification>
            <general>
                <agree>Enter</agree>
            </general>
        </ageverification>
        <ageverification>
            <general>
                <disagree>Cancel</disagree>
            </general>
        </ageverification>
    </default>
</config>
